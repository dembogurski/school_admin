/*Table structure for table `mnt_caja_con` */

DROP TABLE IF EXISTS `mnt_caja_con`;

CREATE TABLE `mnt_caja_con` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `cc_cod` varchar(2) DEFAULT NULL,
  `cc_descri` varchar(40) DEFAULT NULL,
  `cc_oper` varchar(1) DEFAULT NULL,
  `cc_autom` varchar(2) DEFAULT NULL,
  UNIQUE KEY `id` (`id`),
  UNIQUE KEY `cc_cod` (`cc_cod`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

/*Data for the table `mnt_caja_con` */

